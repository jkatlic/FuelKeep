<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.katlic.mpgtracker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.katlic.mpgtracker.MpgTrackerMainActivity"
            android:label="@string/mpg_main_activity_title" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.katlic.mpgtracker.MpgTrackerSettingsActivity"
            android:label="@string/mpg_setting_activity_title" >
        </activity>
        <activity
            android:name="com.katlic.mpgtracker.MpgTrackerAddNewLogActivity"
            android:label="@string/mpg_add_new_activity_title" >
        </activity>
    </application>

</manifest>
